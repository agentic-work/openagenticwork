{
  "name": "@agentic-work/agenticode",
  "version": "0.2.20",
  "description": "AgentiCode - AgenticWork AI Code Assistant CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentic-work/agenticode.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agenticode": "dist/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run copy-skills",
    "copy-skills": "cp -r src/skills/builtin dist/skills/",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@agentic-work/sdk": "file:../sdk",
    "@inkjs/ui": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@types/ink-gradient": "^2.0.4",
    "chalk": "^5.3.0",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "ink": "^5.0.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.0",
    "marked-terminal": "^7.2.1",
    "ora": "^8.1.1",
    "react": "^18.3.1",
    "readline": "^1.3.0",
    "shell-quote": "^1.8.1",
    "simple-git": "^3.27.0",
    "strip-ansi": "^7.1.0",
    "undici": "^7.1.0",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/diff": "^5.2.3",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/shell-quote": "^1.7.5",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
