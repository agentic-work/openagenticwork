{
  "name": "@agentic-work/sdk",
  "version": "0.2.20",
  "description": "AgenticWork SDK - Direct LLM provider access for agentic applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentic-work/agenticwork-sdk.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "openai": "^4.76.0",
    "@google/genai": "^0.7.0",
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.0",
    "openai": ">=4.60.0",
    "@google/genai": ">=0.5.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": { "optional": true },
    "openai": { "optional": true },
    "@google/genai": { "optional": true }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "llm",
    "ai",
    "sdk",
    "anthropic",
    "openai",
    "gemini",
    "ollama",
    "agentic"
  ],
  "author": "AgenticWork",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
